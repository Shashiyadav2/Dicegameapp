{
  "expo": {
    "name": "Dicegame",
    "slug": "dice",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/images/icon.png",
    "scheme": "dice",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "statusBarHidden": true,
    "statusBarStyle": "light-content",
    "splash": {
      "image": "./assets/images/LOGO.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "hideStatusBar": true
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "statusBarHidden": true,
      "statusBarStyle": "light-content",
      "orientations": [
        "landscape-left",
        "landscape-right"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LOGO.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "screenOrientation": "landscape",
      "statusBar": {
        "hidden": true,
        "style": "light-content"
      },
      "navigationBar": {
        "visible": "immersive"
      },
      "package": "com.guru121.dice"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/LOGO.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7ca28735-a25e-49e5-b519-e1adc15f3551"
      }
    }
  }
}
